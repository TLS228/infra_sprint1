[Unit]
Description=gunicorn daemon for Kittygram
After=network.target

[Service]
User=yc-user
Group=www-data
WorkingDirectory=/home/yc-user/infra_sprint1/backend
ExecStart=/home/yc-user/Kittygram/infra_sprint1/venv/bin/gunicorn --workers 3 --bind 0.0.0.0:8080 kittygram.wsgi:application

[Install]
WantedBy=multi-user.target
